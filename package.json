{
  "name": "angular-tour-of-heroes",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "json-server": "nohup node server/json-server.js &",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner",
    "build": "ng build",
    "test": "jest --silent",
    "test:verbose": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --reporters=default --reporters=jest-junit --coverageReporters=text --coverageReporters=html --coverageReporters=cobertura  --coverageReporters=lcov --passWithNoTests"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@briebug/jest-schematic": "^2.1.1",
    "jest-junit": "^10.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "json-server": "^0.16.1",
    "karma-coverage": "^1.1.2",
    "karma-mocha-reporter": "^2.2.5",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^26.0.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^26.0.1",
    "jest-preset-angular": "^8.2.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/test.ts"
    ]
  }
}
